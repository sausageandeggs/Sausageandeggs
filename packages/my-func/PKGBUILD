pkgname=my_func
pkgver=169
pkgrel=1
pkgdesc="My functions"
arch=(any)
url=""
license=('GPL')
groups=('sas')
depends=('tar' 'gvim' 'ranger')
#install=($pkgname.install)
source=($pkgname 'funcdo')
noextract=()
package() {
  cd $srcdir
	install -Dm 644 my_func 		$pkgdir/usr/lib/sas/my_func
	install -Dm 755 funcdo		$pkgdir/usr/lib/sas/funcdo
}

