pkgname=shmakepkg
pkgver=2
pkgrel=1
pkgdesc="Speed up makepkg with tmpfs"
arch=('any')
url=
license=('GPL')
depends=('bash')
source=($pkgname.sh)
build() {
  mkdir -p $pkgdir/usr/bin
  install $pkgname.sh $pkgdir/usr/bin/$pkgname
}
md5sums=('c162964d6576ea4915dc92259b864b3d')
