pkgname=shmakepkg
pkgver=3
pkgrel=1
pkgdesc="Speed up makepkg with tmpfs"
arch=('any')
url=
license=('GPL')
depends=('bash')
source=($pkgname.sh)
build() {
  mkdir -p $pkgdir/usr/bin
  install $pkgname.sh $pkgdir/usr/bin/$pkgname
}
